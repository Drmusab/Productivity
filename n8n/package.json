{
	"name": "n8n-nodes-kanban-app",
	"version": "1.0.0",
	"description": "n8n node for Kanban Task Management Application",
	"keywords": [
		"n8n-community-node-package",
		"kanban",
		"task-management",
		"productivity"
	],
	"license": "MIT",
	"homepage": "https://github.com/Drmusab/Kanban-Routine-Manager",
	"author": {
		"name": "Drmusab",
		"email": "drmusab@example.com"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Drmusab/Kanban-Routine-Manager.git"
	},
        "main": "dist/index.js",
        "types": "dist/index.d.ts",
	"scripts": {
		"build": "tsc && gulp build:icons",
		"dev": "tsc --watch",
		"format": "prettier nodes credentials --write",
		"lint": "eslint \"**/*.ts\" --ignore-pattern dist",
		"lintfix": "eslint \"**/*.ts\" --ignore-pattern dist --fix",
		"prepublishOnly": "npm run build && npm run lint -s"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/KanbanAppApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/KanbanApp/KanbanApp.node.js",
			"dist/nodes/KanbanApp/KanbanAppTrigger.node.js"
		]
	},
	"devDependencies": {
		"@langchain/core": "^0.3.68",
		"@n8n/config": "^1.64.0",
		"@n8n/errors": "^0.5.0",
		"@types/node": "^20.19.25",
		"@typescript-eslint/parser": "^5.45.0",
		"axios": "^1.13.2",
		"eslint": "^8.57.1",
		"eslint-plugin-n8n-nodes-base": "^1.11.0",
		"express": "^5.1.0",
		"form-data": "^4.0.5",
		"gulp": "^4.0.2",
		"n8n-workflow": "^0.120.0",
		"prettier": "^2.7.1",
		"ssh2": "^1.17.0",
		"typescript": "^5.9.3"
	}
}
