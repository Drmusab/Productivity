{
  "name": "@productivity-os/backend",
  "version": "1.0.0",
  "description": "Productivity OS backend API server",
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "build": "tsc",
    "test": "jest",
    "migrate": "ts-node ../../scripts/migrate.ts",
    "migrate:status": "ts-node ../../scripts/migrate.ts status",
    "migrate:rollback": "ts-node ../../scripts/migrate.ts rollback",
    "backup": "ts-node ../../scripts/backup.ts",
    "backup:vault": "ts-node ../../scripts/backup.ts vault",
    "backup:db": "ts-node ../../scripts/backup.ts db",
    "backup:list": "ts-node ../../scripts/backup.ts list",
    "restore": "ts-node ../../scripts/backup.ts restore",
    "seed": "ts-node ../../scripts/seed.ts",
    "seed:minimal": "ts-node ../../scripts/seed.ts minimal",
    "seed:clear": "ts-node ../../scripts/seed.ts clear",
    "worker": "ts-node src/worker.ts"
  },
  "dependencies": {
    "@types/cookie-parser": "^1.4.10",
    "axios": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "marked": "^5.1.2",
    "multer": "^2.0.2",
    "node-cron": "^3.0.2",
    "socket.io": "^4.8.3",
    "sqlite3": "^5.1.6",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "yjs": "^13.6.28"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/express-rate-limit": "^5.1.3",
    "@types/express-validator": "^2.20.33",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.0.3",
    "@types/node-cron": "^3.0.11",
    "@types/socket.io": "^3.0.1",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.4.6",
    "ts-migrate": "^0.1.35",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
